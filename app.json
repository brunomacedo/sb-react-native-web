{
  "expo": {
    "name": "react-native-sb-chiper",
    "slug": "react-native-sb-chiper",
    "main": "node_modules/expo/AppEntry.js",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./public/favicon-32x32.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./public/chiper-brand.svg",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./public/apple-touch-icon.png",
        "backgroundColor": "#ffffff"
      }
    }
  }
}
