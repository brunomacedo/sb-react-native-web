    "babel-loader": "^9.1.2",
    "babel-plugin-react-native-web": "^0.19.6",
    "chromatic": "^6.19.9",
    "cross-env": "^7.0.3",
    "expo": "~48.0.18",
    "expo-status-bar": "~1.4.4",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
    "react-dom": "^18.2.0",
    "react-native-builder-bob": "^0.21.2",
