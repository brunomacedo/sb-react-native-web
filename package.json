    "storybook": "^7.0.26",
    "typescript": "^4.9.4"
  },
  "react-native-builder-bob": {
    "source": "dist",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      [
        "typescript",
        {
          "project": "tsconfig.build.json"
        }
      ]
    ]
  },
  "keywords": [
    "storybook",
    "react-native",
    "react-native-web",
    "shopify/restyle",
    "design-system",
    "expo"
